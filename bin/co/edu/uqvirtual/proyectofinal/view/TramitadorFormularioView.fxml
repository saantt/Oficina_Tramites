      <Button fx:id="btnLlenarFormulario" layoutX="374.0" layoutY="362.0" mnemonicParsing="false" onAction="#llenarFormulario" prefHeight="35.0" prefWidth="148.0" style="-fx-background-color: #198754;" text="LLENAR FORMULARIO" textFill="WHITE">
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
