      <ComboBox fx:id="comboSedeTransito" layoutX="628.0" layoutY="236.0" prefWidth="150.0" promptText="Sede Transito" />
      <Button fx:id="btnGenerarPazSalvo" layoutX="741.0" layoutY="387.0" mnemonicParsing="false" onAction="#generarPazSalvo" prefHeight="35.0" prefWidth="148.0" style="-fx-background-color: #FFFF00;" text="GENERAR PAZ Y SALVO">
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
